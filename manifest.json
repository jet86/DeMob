{
	"name": "DeMob",
	"version": "0.1",
	"description": "Automatically convert mobile URLs to desktop versions.",
	"manifest_version": 2,
	"background": {"scripts":["hostlist.js", "background.js"]},
	"permissions": [
		"<all_urls>",
		"webRequest",
		"webRequestBlocking",
		"storage"
	],
	"icons": {	"16": "icons/DeMob16.png",
				"32": "icons/DeMob32.png",
				"48": "icons/DeMob48.png",
				"128": "icons/DeMob128.png"}
}