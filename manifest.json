{
	"name": "DeMob",
	"version": "0.1",
	"description": "Automatically convert mobile URLs to desktop versions.",
	"manifest_version": 2,
	"background": {"scripts":["hostlist.js", "background.js"]},
	"permissions": [
		"webRequest",
		"*://mobile.reuters.com/*",
		"*://mobile.twitter.com/*",
		"webRequestBlocking",
		"storage"
	]
}